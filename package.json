    "tips"
  ],
  "devDependencies": {
    "browser-sync": "latest",
    "concurrently": "latest",
    "docsify-cli": "latest",
    "gulp": "latest",
    "gulp-autoprefixer": "latest",
    "gulp-concat": "latest",
    "gulp-cssnano": "latest",
    "gulp-sass": "latest",
    "gulp-strip-css-comments": "latest",
    "gulp-uglify": "latest",
    "standard": "latest",
    "standard-markdown": "latest"
  },
  "engines": {
    "node": ">= 6"
  },
  "files": [
    "f"
  ],
  "scripts": {
    "dev": "concurrently \"gulp\" \"npm run server:dev\"",
